Host *
  TCPKeepAlive yes
  ServerAliveInterval 120
  ControlMaster auto
#  ControlPath /tmp/ssh_mux_%h_%p_%r

Host github.com
  User svaksha
  Hostname github.com
  ServerAliveInterval 60
  ControlPersist 1h

Host kheops
  User svaksha
  Hostname kheops.homeunix.org
  ServerAliveInterval 60
  ControlPersist 1h

Host local
  Hostname 127.0.0.1
  User svaksha
  IdentityFile ~/.ssh/id_rsa
  
